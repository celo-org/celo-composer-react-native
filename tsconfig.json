{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*",
        "./*"
      ],
      "@component/*": [
        "./src/components/*"
      ],
      "@componentInterface/*": [
        "./src/components/interface/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
